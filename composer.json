{
	"name": "kdyby/google",
	"type": "library",
	"description": "Google API wrapper and authentication for Nette Framework",
	"keywords": ["nette", "google", "authentication", "auth", "login"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Mikuláš Dítě",
			"email": "mikulas@dite.pro"
		},
		{
			"name": "Filip Procházka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		},
		{
			"name": "Google",
			"homepage": "https://github.com/google/google-api-php-client/graphs/contributors"
		}
	],
	"require": {
		"nette/nette": "~2.1@dev",

		"ext-curl": "*",
		"ext-json": "*",
		"google/apiclient": "~1.0.4@beta"
	},
	"require-dev": {
		"nette/tester": "@dev",
		"jakub-onderka/php-parallel-lint": "~0.6"
	},
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/kdyby/google/issues"
	},
	"autoload": {
		"psr-0": {
			"Kdyby\\Google\\": "src/"
		},
		"classmap": [
			"src/Kdyby/Google/exceptions.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
